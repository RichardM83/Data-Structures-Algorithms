class Solution(object):
    def reverse(self, x):
        str_num = str(x)
        if str_num[0] == "-":
            rev_str_num = "-" + str_num[1:len(str_num)-1:-1]
            return int(rev_str_num)
        else:
            rev_str_num = str_num[::-1]
            return int(rev_str_num)

        if (new_val < -2 ** 31) or (new_val > (2 ** 31) - 1):
            return 0
        return 0